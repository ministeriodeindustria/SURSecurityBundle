{
    "name": "ministeriodeindustria/sur-security-bundle",
    "type": "symfony-bundle",
    "description": "Bundle de seguridad para integracion con el SUR",
    "keywords": ["SUR", "bundle"],
    "homepage": "http://github.com/ministeriodeindustria/SURSecurityBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Sanchez",
            "email": "sanchezalejandro86@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "besimple/soap-bundle" : "0.2.*@dev",
		"besimple/soap-common" : "0.2.*@dev",
		"ass/xmlsecurity" : "1.0.*@dev",
		"besimple/soap-client" : "0.2.*@dev",
		"knplabs/knp-menu-bundle": "2.0.*@dev"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-0": { "SUR\\SecurityBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "SUR/SecurityBundle"
}
